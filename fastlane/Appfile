app_identifiers = ENV["APP_IDENTIFIERS"]
app_identifiers_array = JSON.parse(app_identifiers)
app_identifier(app_identifiers_array) # The bundle identifier of your app

apple_id(ENV["FASTLANE_USER"]) # Your Apple Developer Portal username
itc_team_id(ENV["ITC_TEAM_ID"])
team_id(ENV["TEAM_ID"])

